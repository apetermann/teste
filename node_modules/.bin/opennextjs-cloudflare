#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/ubuntu/teste/node_modules/.pnpm/@opennextjs+cloudflare@0.3.10_wrangler@3.114.4_@cloudflare+workers-types@4.20250409.0_/node_modules/@opennextjs/cloudflare/dist/cli/node_modules:/home/ubuntu/teste/node_modules/.pnpm/@opennextjs+cloudflare@0.3.10_wrangler@3.114.4_@cloudflare+workers-types@4.20250409.0_/node_modules/@opennextjs/cloudflare/dist/node_modules:/home/ubuntu/teste/node_modules/.pnpm/@opennextjs+cloudflare@0.3.10_wrangler@3.114.4_@cloudflare+workers-types@4.20250409.0_/node_modules/@opennextjs/cloudflare/node_modules:/home/ubuntu/teste/node_modules/.pnpm/@opennextjs+cloudflare@0.3.10_wrangler@3.114.4_@cloudflare+workers-types@4.20250409.0_/node_modules/@opennextjs/node_modules:/home/ubuntu/teste/node_modules/.pnpm/@opennextjs+cloudflare@0.3.10_wrangler@3.114.4_@cloudflare+workers-types@4.20250409.0_/node_modules:/home/ubuntu/teste/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/ubuntu/teste/node_modules/.pnpm/@opennextjs+cloudflare@0.3.10_wrangler@3.114.4_@cloudflare+workers-types@4.20250409.0_/node_modules/@opennextjs/cloudflare/dist/cli/node_modules:/home/ubuntu/teste/node_modules/.pnpm/@opennextjs+cloudflare@0.3.10_wrangler@3.114.4_@cloudflare+workers-types@4.20250409.0_/node_modules/@opennextjs/cloudflare/dist/node_modules:/home/ubuntu/teste/node_modules/.pnpm/@opennextjs+cloudflare@0.3.10_wrangler@3.114.4_@cloudflare+workers-types@4.20250409.0_/node_modules/@opennextjs/cloudflare/node_modules:/home/ubuntu/teste/node_modules/.pnpm/@opennextjs+cloudflare@0.3.10_wrangler@3.114.4_@cloudflare+workers-types@4.20250409.0_/node_modules/@opennextjs/node_modules:/home/ubuntu/teste/node_modules/.pnpm/@opennextjs+cloudflare@0.3.10_wrangler@3.114.4_@cloudflare+workers-types@4.20250409.0_/node_modules:/home/ubuntu/teste/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@opennextjs/cloudflare/dist/cli/index.js" "$@"
else
  exec node  "$basedir/../@opennextjs/cloudflare/dist/cli/index.js" "$@"
fi
